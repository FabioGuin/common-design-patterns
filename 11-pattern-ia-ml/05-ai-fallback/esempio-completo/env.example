APP_NAME="AI Fallback Example"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=ai_fallback_example
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=file
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# AI Fallback Configuration
AI_FALLBACK_ENABLED=true
AI_FALLBACK_DEFAULT_STRATEGY=provider_chain
AI_FALLBACK_TIMEOUT=30
AI_FALLBACK_MAX_RETRIES=3

# Circuit Breaker Configuration
AI_FALLBACK_CIRCUIT_BREAKER_ENABLED=true
AI_FALLBACK_FAILURE_THRESHOLD=5
AI_FALLBACK_RECOVERY_TIMEOUT=60
AI_FALLBACK_HALF_OPEN_MAX_CALLS=3
AI_FALLBACK_SUCCESS_THRESHOLD=2

# Retry Configuration
AI_FALLBACK_RETRY_ENABLED=true
AI_FALLBACK_MAX_ATTEMPTS=3
AI_FALLBACK_BACKOFF_STRATEGY=exponential
AI_FALLBACK_BASE_DELAY=1000
AI_FALLBACK_MAX_DELAY=30000
AI_FALLBACK_JITTER=true

# Health Monitoring Configuration
AI_FALLBACK_HEALTH_MONITORING_ENABLED=true
AI_FALLBACK_HEALTH_CHECK_INTERVAL=60
AI_FALLBACK_HEALTH_TIMEOUT=10
AI_FALLBACK_HEALTH_FAILURE_THRESHOLD=3
AI_FALLBACK_HEALTH_SUCCESS_THRESHOLD=2

# Error Classification Configuration
AI_FALLBACK_ERROR_CLASSIFICATION_ENABLED=true

# Logging Configuration
AI_FALLBACK_LOGGING_ENABLED=true
AI_FALLBACK_LOG_LEVEL=info
AI_FALLBACK_LOG_FAILURES=true
AI_FALLBACK_LOG_SUCCESSES=false
AI_FALLBACK_LOG_RETRIES=true
AI_FALLBACK_LOG_CIRCUIT_BREAKER=true
AI_FALLBACK_LOG_HEALTH_CHECKS=true
AI_FALLBACK_LOG_RETENTION_DAYS=30

# Cache Configuration
AI_FALLBACK_CACHE_ENABLED=true
AI_FALLBACK_CACHE_DRIVER=redis
AI_FALLBACK_CACHE_TTL=3600
AI_FALLBACK_CACHE_KEY_PREFIX=ai_fallback:
AI_FALLBACK_CACHE_COMPRESS=true
AI_FALLBACK_CACHE_ENCRYPT=false

# Queue Configuration
AI_FALLBACK_QUEUE_ENABLED=true
AI_FALLBACK_QUEUE_CONNECTION=redis
AI_FALLBACK_QUEUE_NAME=ai-processing
AI_FALLBACK_QUEUE_DELAY=60
AI_FALLBACK_QUEUE_MAX_ATTEMPTS=3
AI_FALLBACK_QUEUE_TIMEOUT=300
AI_FALLBACK_QUEUE_RETRY_AFTER=90

# Performance Configuration
AI_FALLBACK_PARALLEL_REQUESTS=false
AI_FALLBACK_MAX_CONCURRENT_REQUESTS=5
AI_FALLBACK_REQUEST_TIMEOUT=30
AI_FALLBACK_RESPONSE_TIMEOUT=60
AI_FALLBACK_MEMORY_LIMIT=256M
AI_FALLBACK_ENABLE_COMPRESSION=true

# Security Configuration
AI_FALLBACK_ENCRYPT_RESPONSES=false
AI_FALLBACK_ENCRYPTION_KEY=
AI_FALLBACK_SANITIZE_INPUT=true
AI_FALLBACK_RATE_LIMITING_ENABLED=true
AI_FALLBACK_MAX_REQUESTS_PER_MINUTE=60
AI_FALLBACK_MAX_REQUESTS_PER_HOUR=1000

# Monitoring Configuration
AI_FALLBACK_MONITORING_ENABLED=true
AI_FALLBACK_METRICS_COLLECTION=true
AI_FALLBACK_ALERTING_ENABLED=true
AI_FALLBACK_FAILURE_RATE_THRESHOLD=0.5
AI_FALLBACK_RESPONSE_TIME_THRESHOLD=10
AI_FALLBACK_DASHBOARDS_ENABLED=true
AI_FALLBACK_REAL_TIME_UPDATES=true
AI_FALLBACK_AUTO_REFRESH_INTERVAL=30

# Fallback Configuration
AI_FALLBACK_GRACEFUL_DEGRADATION=true
AI_FALLBACK_FALLBACK_TO_STATIC=true
AI_FALLBACK_FALLBACK_TO_CACHE=true
AI_FALLBACK_FALLBACK_TO_QUEUE=true
AI_FALLBACK_MAX_FALLBACK_DEPTH=3
AI_FALLBACK_FALLBACK_TIMEOUT=120

# AI Provider API Keys
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_BASE_URL=https://api.anthropic.com

GOOGLE_AI_API_KEY=your_google_ai_api_key_here
GOOGLE_AI_BASE_URL=https://generativelanguage.googleapis.com

HUGGINGFACE_API_KEY=your_huggingface_api_key_here
HUGGINGFACE_BASE_URL=https://api-inference.huggingface.co
