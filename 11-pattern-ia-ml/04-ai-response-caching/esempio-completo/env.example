APP_NAME="AI Cache Example"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=ai_cache_example
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# AI Cache Configuration
AI_CACHE_ENABLED=true
AI_CACHE_DEFAULT_STRATEGY=lru
AI_CACHE_DEFAULT_TTL=3600
AI_CACHE_MAX_SIZE=10000
AI_CACHE_COMPRESSION_ENABLED=true
AI_CACHE_COMPRESSION_THRESHOLD=1024

# Redis Configuration for AI Cache
AI_CACHE_REDIS_ENABLED=true
AI_CACHE_REDIS_CONNECTION=default
AI_CACHE_REDIS_PREFIX=ai_cache:

# Memcached Configuration for AI Cache
AI_CACHE_MEMCACHED_ENABLED=false

# Database Configuration for AI Cache
AI_CACHE_DATABASE_ENABLED=false

# File Configuration for AI Cache
AI_CACHE_FILE_ENABLED=false

# Cache Warming Configuration
AI_CACHE_WARMING_ENABLED=true
AI_CACHE_ASYNC_WARMING=true

# Analytics Configuration
AI_CACHE_ANALYTICS_ENABLED=true
AI_CACHE_ANALYTICS_RETENTION=90

# Compression Configuration
AI_CACHE_COMPRESSION_ALGORITHM=gzip
AI_CACHE_COMPRESSION_LEVEL=6

# Security Configuration
AI_CACHE_ENCRYPT_SENSITIVE=true
AI_CACHE_HASH_KEYS=true
AI_CACHE_KEY_SALT=ai_cache_salt
AI_CACHE_ENCRYPTION_KEY=

# Performance Configuration
AI_CACHE_LAZY_LOADING=true
AI_CACHE_PREFETCH_ENABLED=true
AI_CACHE_BATCH_OPERATIONS=true
AI_CACHE_CONNECTION_POOLING=true

# Monitoring Configuration
AI_CACHE_MONITORING_ENABLED=true
AI_CACHE_LOG_OPERATIONS=true
AI_CACHE_LOG_LEVEL=info
AI_CACHE_ALERT_RECIPIENTS=
AI_CACHE_WEBHOOK_URL=

# Cost Optimization Configuration
AI_CACHE_COST_OPTIMIZATION_ENABLED=true
AI_CACHE_API_COST_PER_REQUEST=0.01
AI_CACHE_COST_PER_MB=0.001
AI_CACHE_AUTO_EVICTION=true
AI_CACHE_COST_TRACKING=true

# Cache Tags Configuration
AI_CACHE_TAGS_ENABLED=true
AI_CACHE_AUTO_CLEANUP_TAGS=true

# Fallback Configuration
AI_CACHE_FALLBACK_ENABLED=true
AI_CACHE_FALLBACK_DRIVER=file
AI_CACHE_CIRCUIT_BREAKER=true
