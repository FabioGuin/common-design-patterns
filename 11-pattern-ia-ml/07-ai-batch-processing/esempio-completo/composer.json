{
    "name": "laravel/ai-batch-processing-example",
    "type": "project",
    "description": "Esempio di AI Batch Processing Pattern per elaborazione batch di richieste AI",
    "keywords": [
        "laravel",
        "ai",
        "batch-processing",
        "openai",
        "claude",
        "gemini"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "guzzlehttp/guzzle": "^7.8",
        "openai-php/client": "^0.7.0",
        "anthropic-php/sdk": "^0.7.0",
        "google/generative-ai": "^0.2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
