APP_NAME="Caching Strategies Example"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# Cache Configuration
CACHE_STORE=redis
CACHE_PREFIX=cache_strategies
CACHE_TTL=3600

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DB=0

# Memcached Configuration (alternative)
MEMCACHED_HOST=127.0.0.1
MEMCACHED_PORT=11211

# Cache Strategies Settings
CACHE_STRATEGY=write_through
CACHE_TAGS=true
CACHE_WARMUP=true

# Performance Settings
CACHE_COMPRESSION=true
CACHE_SERIALIZATION=php

# Database Configuration
DB_CONNECTION=sqlite
DB_DATABASE=database/database.sqlite

# Queue Configuration
QUEUE_CONNECTION=redis
