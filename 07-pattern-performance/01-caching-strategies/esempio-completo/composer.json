{
    "name": "laravel/caching-strategies-example",
    "type": "project",
    "description": "Esempio di Caching Strategies Pattern per ottimizzazione performance",
    "keywords": [
        "laravel",
        "caching",
        "performance",
        "redis",
        "memcached"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "predis/predis": "^2.2",
        "laravel/horizon": "^5.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
